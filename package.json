{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "rust-docs-bot",
  "description": "A Discord bot for fetching the Rust language-related documentation.",
  "author": "Danial Raza <danialrazafb@gmail.com>",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "commitlint": "commitlint --edit",
    "dev": "pnpm build && pnpm start",
    "deploy": "pnpm build && node --env-file=.env dist/deploy-commands.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix src",
    "lint:check": "eslint src",
    "prepare": "is-ci || husky",
    "start": "node --env-file=.env dist/index.js"
  },
  "dependencies": {
    "@discordjs/core": "^2.3.0",
    "closest-match": "^1.3.3",
    "discord.js": "^14.24.0",
    "leven-sort": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-angular": "^20.0.0",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "eslint-config-neon": "^0.2.9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=24",
    "pnpm": "10.19.0"
  },
  "packageManager": "pnpm@10.19.0",
  "volta": {
    "node": "24.10.0",
    "pnpm": "10.19.0"
  }
}
